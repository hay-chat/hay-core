{
  "$schema": "../base/plugin-manifest.schema.json",
  "id": "judo-in-cloud",
  "name": "Judo in Cloud",
  "description": "Judo in Cloud MCP plugin",
  "version": "1.0.0",
  "author": "Hay",
  "type": ["mcp-connector"],
  "entry": "./dist/index.js",
  "enabled": true,
  "category": "utility",
  "icon": "cloud",
  "marketplace": {
    "featured": false,
    "tags": ["judo", "password-reset", "email", "contact", "utility"]
  },
  "permissions": {
    "api": ["mcp"],
    "env": ["JUDO_IN_CLOUD_API_KEY"],
    "scopes": ["org:<organizationId>:mcp:invoke"]
  },
  "configSchema": {
    "judoInCloudApiKey": {
      "type": "string",
      "description": "API key for the Judo in Cloud API",
      "label": "Judo in Cloud API Key",
      "placeholder": "Enter your Judo in Cloud API key",
      "required": true,
      "encrypted": true,
      "env": "JUDO_IN_CLOUD_API_KEY"
    }
  },
  "capabilities": ["mcp"],
  "ui": {
    "settings": false
  }
}
