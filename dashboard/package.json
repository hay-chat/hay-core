{
  "name": "@hay/dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "fuser -k 5173/tcp 2>/dev/null || true && nuxt dev",
    "dev:clean": "fuser -k 5173/tcp 2>/dev/null || true && rm -rf .nuxt && nuxt dev",
    "build": "nuxt build",
    "build:prod": "NODE_ENV=production nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "typecheck": "nuxt typecheck",
    "postinstall": "nuxt prepare",
    "start": "cd .output && node server/index.mjs"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.30.8",
    "@editorjs/list": "^2.0.8",
    "editorjs-mention-tool": "^1.0.2",
    "editorjs-undo": "^2.0.28"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^9.0.8"
  }
}
