{
  "name": "@hay/dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "fuser -k 3000/tcp 2>/dev/null || true && nuxt dev",
    "dev:clean": "fuser -k 3000/tcp 2>/dev/null || true && rm -rf .nuxt && nuxt dev",
    "build": "nuxt build",
    "build:prod": "NODE_ENV=production nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "generate:trpc": "npx tsx scripts/generate-trpc-types.ts",
    "typecheck": "nuxt typecheck",
    "postinstall": "nuxt prepare",
    "start": "cd .output && node server/index.mjs"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^9.0.8",
    "sass": "^1.92.0"
  },
  "dependencies": {
    "@tiptap/extension-mention": "^2.26.3",
    "@tiptap/extension-placeholder": "^2.10.5",
    "@tiptap/pm": "^2.10.5",
    "@tiptap/starter-kit": "^2.10.5",
    "@tiptap/suggestion": "^2.10.5",
    "@tiptap/vue-3": "^2.10.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "sonner": "^2.0.7",
    "tippy.js": "^6.3.7",
    "vue-json-pretty": "^2.5.0",
    "vue-shepherd": "^5.0.1"
  }
}
